# Environment Configuration
# These variables override settings in config.js

# Server settings
PORT=3000

# Image source prefix
PREFIX=https://

# Image processing limits
MAX_WIDTH=4096
MAX_HEIGHT=4096
DEFAULT_QUALITY=85
FETCH_TIMEOUT=10000

# Cache settings
CACHE_CONTROL="public, max-age=31536000, immutable"

# CORS settings
CORS_ORIGIN=*
CORS_CREDENTIALS=false

# Presets (JSON format)
# PRESETS={"thumbnail":"w=200,h=200,fit=cover","banner":"w=1024,h=128"}
